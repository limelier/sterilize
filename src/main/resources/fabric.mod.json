{
  "schemaVersion": 1,
  "id": "sterilize",
  "version": "${version}",

  "name": "Sterilize",
  "description": "frick vines",
  "authors": [
    "limelier"
  ],
  "contact": {
    "homepage": "https://limelier.me",
    "sources": "https://github.com/limelier/sterilize"
  },

  "license": "MIT",
  "icon": "assets/sterilize/icon.png",

  "environment": "*",
  "entrypoints": {
    "cardinal-components": [
      {
        "adapter": "kotlin",
        "value": "me.limelier.sterilize.ChunkFertileBlocksComponentInitializer"
      }
    ]
  },
  "mixins": [
    "sterilize.mixins.json"
  ],
  "custom": {
    "cardinal-components": [
      "sterilize:chunk-fertile-blocks-component"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.8",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.8.1+kotlin.1.7.0"
  }
}
